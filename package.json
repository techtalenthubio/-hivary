{
  "name": "@hivary/engine-utils",
  "version": "1.0.0",
  "description": "Public Packages that are used by the Hivary Engine",
  "author": "hivary",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "rimraf dist && tsc",
    "prepare": "npm run build && npm pack",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "hivary"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/techtalenthubio/engine-utils.git"
  },
  "bugs": {
    "url": "https://github.com/techtalenthubio/engine-utils/issues"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.3.0"
  },
  "devDependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/testing": "^10.3.0",
    "@types/express": "4.16.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "jest": "^29.7.0",
    "nestjs-i18n": "^10.4.0",
    "prettier": "1.17.0",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "supertest": "4.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/techtalenthubio/engine-utils#readme"
}
